  "version": "0.2.0",
  "configurations": [
    {
      "name": "開発版デバッグビルド",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "5000",
        "--web-renderer",
        "html",
        "--dart-define=FLAVOR=dev"
      ]
    },
    {
      "name": "本番版デバッグビルド",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "5000",
        "--web-renderer",
        "html",
        "--dart-define=FLAVOR=prod"
      ]
    },
    {
      "name": "本番版リリースビルド",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "5000",
        "--web-renderer",
        "html",
        "--dart-define=FLAVOR=prod"
      ],
      "flutterMode": "release"
    }
  ]
}
